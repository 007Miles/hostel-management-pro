<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ManageAccommodation">

    <include layout="@layout/toolbar"
        android:id="@+id/toolbar"/>

    <Button
        android:id="@+id/btnMA"
        android:layout_width="306dp"
        android:layout_height="127dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="53dp"
        android:backgroundTint="@color/blue"
        android:text="@string/manage_accommodation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        tools:ignore="TextContrastCheck,UsingOnClickInXml"
        android:onClick="onclickMA" />


    <Button
        android:id="@+id/btnRD"
        android:layout_width="306dp"
        android:layout_height="127dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="53dp"
        android:backgroundTint="@color/blue"
        android:text="@string/room_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMA"
        tools:ignore="TextContrastCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>